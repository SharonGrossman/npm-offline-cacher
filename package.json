{
  "name": "npm-offline-cacher",
  "version": "0.1.0",
  "description": "Npm caching offline",
  "main": "lib/index.js",
  "bin": {
    "npm-offline-cacher": "bin/npm-offline-cacher",
    "noc": "bin/npm-offline-cacher"
  },
  "scripts": {
    "lint": "eslint src --fix",
    "babel": "babel src -d lib -s",
    "build": "rimraf lib && yarn babel && yarn link",
    "prepublishOnly": "yarn build",
    "test": "yarn lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SharonGrossman/npm-offline-cacher.git"
  },
  "keywords": [
    "npm",
    "offline",
    "caching"
  ],
  "author": "Sharon Grossman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SharonGrossman/npm-offline-cacher/issues"
  },
  "homepage": "https://github.com/SharonGrossman/npm-offline-cacher#readme",
  "module": "src/index.js",
  "engines": {
    "node": ">=8"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "caporal": "^1.1.0",
    "eslint": "^5.8.0",
    "eslint-config-noamokman": "^8.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-lodash": "^3.1.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "rimraf": "^2.6.2",
    "update-notifier": "^2.5.0",
    "yarn": "^1.13.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "npm": "^6.9.0",
    "pify": "^4.0.1",
    "verdaccio": "^3.11.6"
  }
}
